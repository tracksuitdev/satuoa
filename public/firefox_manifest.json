{
  "name": "satuoa",
  "version": "1.0",
  "description": "Denominate prices in sats",
  "permissions": [],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo/sat-symbol-16.png",
      "32": "images/logo/sat-symbol-32.png",
      "48": "images/logo/sat-symbol-48.png",
      "128": "images/logo/sat-symbol-128.png"
    },
    "default_title": "SATUOA"
  },
  "icons": {
    "16": "images/logo/sat-symbol-16.png",
    "32": "images/logo/sat-symbol-32.png",
    "48": "images/logo/sat-symbol-48.png",
    "128": "images/logo/sat-symbol-128.png"
  }
}
